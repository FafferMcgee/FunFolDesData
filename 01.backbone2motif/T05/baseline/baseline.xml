<ROSETTASCRIPTS>
	<RESIDUE_SELECTORS>
		<Layer name="core1" select_core="true" use_sidechain_neighbors="false" />
    <Layer name="core2" select_core="true" use_sidechain_neighbors="true" />
		<Or name="core" selectors="core1,core2" />

    <Index name="picked" resnums="8A,15A,16A,24A,34A,37A,38A,41A,42A,45A,46A,80A,92A" />

    <Index name="motif" resnums="49A-64A" />
		<Index name="query" resnums="32A-48A" />

	</RESIDUE_SELECTORS>
	<FILTERS>
	</FILTERS>
	<MOVERS>
    <WriteSSEMover dssp="1" name="writesse" />
    <LabelPoseFromResidueSelectorMover name="corelab" property="CORE" residue_selector="core" />
		<LabelPoseFromResidueSelectorMover name="pickedlab" property="PICKED" residue_selector="picked" />
		<LabelPoseFromResidueSelectorMover name="motiflab" property="MOTIF" residue_selector="motif" />
    <LabelPoseFromResidueSelectorMover name="querylab" property="QUERY" residue_selector="query" />
    <DisplayPoseLabelsMover name="write" write="true" />
    <ContactMap name="contacts" prefix="contacts_" />
	</MOVERS>
	<PROTOCOLS>
    <Add mover="writesse" />
    <Add mover="corelab" />
		<Add mover="pickedlab" />
		<Add mover="motiflab" />
    <Add mover="querylab" />
    <Add mover="write" />
    <Add mover="contacts" />
	</PROTOCOLS>
</ROSETTASCRIPTS>